<h1>Microsoft Graph API</h1>
<h2>Your Calendar</h2>
{{#if calendar}}
  <table>
    <thead>
      <tr>
        <th>Subject</th>
        <th>Start</th>
        <th>End</th>
      </tr>
    </thead>
    <tbody>
      {{#each calendar}}
        <tr>
          <td>{{subject}}</td>
          <td>{{start.dateTime}}</td>
          <td>{{end.dateTime}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p>No events found.</p>
{{/if}}